  "scripts": {
    "build": "next build",
    "db:seed": "pnpx tsx prisma/seed.tsx",
    "db:push": "pnpm prisma db push",
    "db:pull": "pnpm prisma db pull",
    "db:generate": "pnpm prisma generate",
    "db:migrate:dev": "pnpm prisma migrate dev",
    "db:migrate:reset": "pnpm prisma migrate reset",
    "db:migrate:deploy": "pnpm prisma migrate deploy",
    "db:studio": "pnpm prisma studio",
    "dev": "next dev",
    "dev:secure": "next dev --experimental-https",
    "dev:fast": "next dev --experimental-https --turbo",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe --latest",
    "test:e2e": "playwright test",
    "pre-deploy": "pnpm typecheck && pnpm lint && pnpm test:e2e && pnpm build",
    "dev:db:push": "bash ./scripts/bash/dev-push.sh",
    "dev:db:migrate": "bash ./scripts/bash/dev-migration.sh"
  },
